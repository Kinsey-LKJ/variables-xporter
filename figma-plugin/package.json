    "@texel/color": "^1.1.9",
    "@types/react-resizable": "^3.0.8",
    "change-case": "^5.4.4",
    "colorjs.io": "^0.5.2",
    "embla-carousel-auto-height": "^8.0.0",
    "embla-carousel-react": "^8.0.0",
    "figx": "^0.1.3",
    "gsap": "^3.12.5",
    "lucide-react": "^0.359.0",
    "mantine-form-zod-resolver": "^1.1.0",
    "parse-github-url": "^1.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
